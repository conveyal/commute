{
  "name": "@conveyal/commute",
  "description": "Commute analysis",
  "scripts": {
    "build": "mastarm build client/index.js:assets/index.js client/index.css:assets/index.css",
    "deploy": "mastarm deploy client/index.js:assets/index.js client/index.css:assets/index.css",
    "cover": "npm run cover-client && npm run cover-server && npm run merge-coverage",
    "precover-client": "rm -rf coverage",
    "cover-client": "npm run test-client -- --coverage --coverage-paths \"client/**/*.js\"",
    "postcover-client": "mv coverage coverage-client",
    "precover-client": "rm -rf coverage",
    "cover-server": "npm run test-server -- --coverage --coverage-paths \"server/**/*.js\"",
    "postcover-client": "mv coverage coverage-server",
    "premerge-coverage": "rm -rf coverage && mkdir coverage",
    "merge-coverage": "./node_modules/.bin/lcov-result-merger \"coverage-*/lcov.info\" \"coverage/lcov.info\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "mastarm build client/index.js:assets/index.js client/index.css:assets/index.css --watch",
    "start:server": "nodemon --watch server server/development.js",
    "test": "npm run test-lint && npm run test-client && npm run test-server",
    "test-client": "mastarm test -c ./configurations/test -e test __tests__/client",
    "test-lint": "mastarm lint",
    "test-server": "mastarm test -e test __tests__/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/commute.git"
  },
  "keywords": [
    "Modeify",
    "tdm"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/commute/issues"
  },
  "homepage": "https://github.com/conveyal/commute#readme",
  "devDependencies": {
    "buffer-equal-constant-time": "^1.0.1",
    "enzyme": "^2.5.1",
    "enzyme-to-json": "^1.1.4",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "express-jwt": "^5.1.0",
    "fs-extra": "^0.30.0",
    "lcov-result-merger": "^1.2.0",
    "mastarm": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "pretty": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "d3-dsv": "^1.0.3",
    "font-awesome": "^4.6.3",
    "graphql": "^0.7.2",
    "humanize-duration": "^3.9.1",
    "leaflet": "^0.7.7",
    "lodash.isequal": "^4.4.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.uniqueid": "^4.0.1",
    "lonlng": "^0.2.0",
    "moment": "^2.15.2",
    "mongoose": "^4.6.4",
    "normalize.css": "^5.0.0",
    "rc-slider": "^5.2.1",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-bootstrap-table": "^2.5.6",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.7.2",
    "react-leaflet": "^0.12.3",
    "react-leaflet-cluster-layer": "^0.0.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-select": "^1.0.0-rc.2",
    "react-select-geocoder": "^0.2.1",
    "react-vis": "^0.6.4",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "uuid": "^2.0.3"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
